<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <entry key="analyzer.executable"/>
    <entry key="analyzer.runInEnvironmentContext">false</entry>
    <entry key="analyzer.options.cppcheck">--std=posix --enable=all</entry>
    <entry key="analyzer.options.pclint">-width(255,4) -hf1 -format="%f:%l: %t %n: %m" -b</entry>
</properties>
